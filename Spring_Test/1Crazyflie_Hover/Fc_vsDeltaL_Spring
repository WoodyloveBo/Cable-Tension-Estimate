import matplotlib.pyplot as plt

# Data
F_c = [10.1, 10.96, 12.08, 12.428, 13.67, 15.5, 16.08, 17.34, 17.7, 19.26, 20.48]
delta_L = [0.053, 0.06, 0.072, 0.077, 0.091, 0.112, 0.116, 0.132, 0.139, 0.157, 0.17]

# Create the plot
plt.figure()
plt.plot(F_c, delta_L, marker='o')
plt.xlabel('Weight (g)')
plt.ylabel('ΔL (m)')
plt.title('ΔL vs Weight')
plt.grid(True)
plt.show()